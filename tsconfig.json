{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "circle-chart": [
        "dist/circle-chart"
      ],
      "circle-chart/*": [
        "dist/circle-chart/*"
      ],
      "line-chart": [
        "dist/line-chart"
      ],
      "line-chart/*": [
        "dist/line-chart/*"
      ],
      "pie-chart": [
        "dist/pie-chart"
      ],
      "pie-chart/*": [
        "dist/pie-chart/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}